<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BotData" table="bot_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="appVersion" type="text" column="app_version" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="string" column="language" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hit" type="string" column="hit" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="string" column="position" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ipAddress" type="string" column="ip_address" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="geo" type="string" column="geo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
