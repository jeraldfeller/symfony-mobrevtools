<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ReportsConversionsImported" table="reports_conversions_imported">
    <indexes>
      <index name="igid" columns="igid"/>
    </indexes>
    <id name="ctid" type="integer" column="ctid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="campaignname" type="string" column="campaignName" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="campaignid" type="string" column="campaignId" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ipint" type="integer" column="ipInt" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visits" type="integer" column="visits" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="conversions" type="integer" column="conversions" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="revenue" type="float" column="revenue" precision="10" scale="0" nullable="false"/>
    <field name="cost" type="float" column="cost" precision="10" scale="0" nullable="false"/>
    <field name="profit" type="float" column="profit" precision="10" scale="0" nullable="false"/>
    <field name="roi" type="float" column="roi" precision="10" scale="0" nullable="false"/>
    <field name="countrycode" type="string" column="countryCode" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryname" type="string" column="countryName" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isp" type="string" column="isp" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mobilecarrier" type="string" column="mobileCarrier" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="connectiontype" type="string" column="connectionType" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visittimestamp" type="date" column="visitTimestamp" nullable="false"/>
    <many-to-one field="igid" target-entity="ImportGroup" fetch="LAZY">
      <join-columns>
        <join-column name="igid" referenced-column-name="igid"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
