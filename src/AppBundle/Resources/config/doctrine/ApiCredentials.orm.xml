<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ApiCredentials" table="api_credentials">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="voluum" type="string" column="voluum" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zeropark" type="string" column="zeropark" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="exoclick" type="string" column="exoclick" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
