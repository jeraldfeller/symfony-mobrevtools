<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ReportsIp" table="reports_ip">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="trafficName" type="string" column="traffic_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="campaignName" type="string" column="campaign_name" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="geo" type="string" column="geo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mobileCarrier" type="string" column="mobile_carrier" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
