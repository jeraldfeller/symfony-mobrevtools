<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ReportsBlacklist" table="reports_blacklist">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="blid" type="integer" column="blid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tid" type="integer" column="tid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cgid" type="integer" column="cgid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="trafficSource" type="string" column="traffic_source" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="geo" type="string" column="geo" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vertical" type="string" column="vertical" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="placement" type="string" column="placement" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="visits" type="float" column="visits" precision="10" scale="0" nullable="false"/>
    <field name="clicks" type="integer" column="clicks" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ctr" type="float" column="ctr" precision="10" scale="0" nullable="false"/>
    <field name="cost" type="float" column="cost" precision="10" scale="0" nullable="false"/>
    <field name="roi" type="float" column="roi" precision="10" scale="0" nullable="false"/>
    <field name="conversions" type="float" column="conversions" precision="10" scale="0" nullable="false"/>
    <field name="campaignName" type="string" column="campaign_name" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="frequency" type="integer" column="frequency" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="startFrom" type="integer" column="start_from" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateExecuted" type="integer" column="date_executed" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
