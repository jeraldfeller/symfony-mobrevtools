<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdplexityAutomatedSettings" table="adplexity_automated_settings">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aggregation" type="string" column="aggregation" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="metric" type="string" column="metric" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateRange" type="string" column="date_range" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="daysRunning" type="string" column="days_running" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countries" type="text" column="countries" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adType" type="string" column="ad_type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="affiliateNetwork" type="text" column="affiliate_network" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="trafficSource" type="text" column="traffic_source" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
