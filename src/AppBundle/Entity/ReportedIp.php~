<?php

namespace AppBundle\Entity;

/**
 * ReportedIp
 */
class ReportedIp
{
    /**
     * @var string
     */
    private $ip;

    /**
     * @var string
     */
    private $carrier;

    /**
     * @var integer
     */
    private $repid;


    /**
     * Set ip
     *
     * @param string $ip
     *
     * @return ReportedIp
     */
    public function setIp($ip)
    {
        $this->ip = $ip;

        return $this;
    }

    /**
     * Get ip
     *
     * @return string
     */
    public function getIp()
    {
        return $this->ip;
    }

    /**
     * Set carrier
     *
     * @param string $carrier
     *
     * @return ReportedIp
     */
    public function setCarrier($carrier)
    {
        $this->carrier = $carrier;

        return $this;
    }

    /**
     * Get carrier
     *
     * @return string
     */
    public function getCarrier()
    {
        return $this->carrier;
    }

    /**
     * Get repid
     *
     * @return integer
     */
    public function getRepid()
    {
        return $this->repid;
    }
}
