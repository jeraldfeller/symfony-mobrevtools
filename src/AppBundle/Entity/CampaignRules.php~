<?php

namespace AppBundle\Entity;

/**
 * CampaignRules
 */
class CampaignRules
{
    /**
     * @var string
     */
    private $ruleType;

    /**
     * @var \DateTime
     */
    private $startFrom;

    /**
     * @var integer
     */
    private $frequency;

    /**
     * @var string
     */
    private $operator;

    /**
     * @var boolean
     */
    private $active;

    /**
     * @var \DateTime
     */
    private $dateUpdated;

    /**
     * @var integer
     */
    private $campaignRulesId;

    /**
     * @var \AppBundle\Entity\Campaign
     */
    private $cid;


    /**
     * Set ruleType
     *
     * @param string $ruleType
     *
     * @return CampaignRules
     */
    public function setRuleType($ruleType)
    {
        $this->ruleType = $ruleType;

        return $this;
    }

    /**
     * Get ruleType
     *
     * @return string
     */
    public function getRuleType()
    {
        return $this->ruleType;
    }

    /**
     * Set startFrom
     *
     * @param \DateTime $startFrom
     *
     * @return CampaignRules
     */
    public function setStartFrom($startFrom)
    {
        $this->startFrom = $startFrom;

        return $this;
    }

    /**
     * Get startFrom
     *
     * @return \DateTime
     */
    public function getStartFrom()
    {
        return $this->startFrom;
    }

    /**
     * Set frequency
     *
     * @param integer $frequency
     *
     * @return CampaignRules
     */
    public function setFrequency($frequency)
    {
        $this->frequency = $frequency;

        return $this;
    }

    /**
     * Get frequency
     *
     * @return integer
     */
    public function getFrequency()
    {
        return $this->frequency;
    }

    /**
     * Set operator
     *
     * @param string $operator
     *
     * @return CampaignRules
     */
    public function setOperator($operator)
    {
        $this->operator = $operator;

        return $this;
    }

    /**
     * Get operator
     *
     * @return string
     */
    public function getOperator()
    {
        return $this->operator;
    }

    /**
     * Set active
     *
     * @param boolean $active
     *
     * @return CampaignRules
     */
    public function setActive($active)
    {
        $this->active = $active;

        return $this;
    }

    /**
     * Get active
     *
     * @return boolean
     */
    public function getActive()
    {
        return $this->active;
    }

    /**
     * Set dateUpdated
     *
     * @param \DateTime $dateUpdated
     *
     * @return CampaignRules
     */
    public function setDateUpdated($dateUpdated)
    {
        $this->dateUpdated = $dateUpdated;

        return $this;
    }

    /**
     * Get dateUpdated
     *
     * @return \DateTime
     */
    public function getDateUpdated()
    {
        return $this->dateUpdated;
    }

    /**
     * Get campaignRulesId
     *
     * @return integer
     */
    public function getCampaignRulesId()
    {
        return $this->campaignRulesId;
    }

    /**
     * Set cid
     *
     * @param \AppBundle\Entity\Campaign $cid
     *
     * @return CampaignRules
     */
    public function setCid(\AppBundle\Entity\Campaign $cid = null)
    {
        $this->cid = $cid;

        return $this;
    }

    /**
     * Get cid
     *
     * @return \AppBundle\Entity\Campaign
     */
    public function getCid()
    {
        return $this->cid;
    }
    /**
     * @var integer
     */
    private $datetimeunix;


    /**
     * Set datetimeunix
     *
     * @param integer $datetimeunix
     *
     * @return CampaignRules
     */
    public function setDatetimeunix($datetimeunix)
    {
        $this->datetimeunix = $datetimeunix;

        return $this;
    }

    /**
     * Get datetimeunix
     *
     * @return integer
     */
    public function getDatetimeunix()
    {
        return $this->datetimeunix;
    }
    /**
     * @var string
     */
    private $dateTime;


    /**
     * Set dateTime
     *
     * @param string $dateTime
     *
     * @return CampaignRules
     */
    public function setDateTime($dateTime)
    {
        $this->dateTime = $dateTime;

        return $this;
    }

    /**
     * Get dateTime
     *
     * @return string
     */
    public function getDateTime()
    {
        return $this->dateTime;
    }
}
