<?php

namespace AppBundle\Entity;

/**
 * BotRules
 */
class BotRules
{
    /**
     * @var integer
     */
    private $opt;

    /**
     * @var integer
     */
    private $visits;

    /**
     * @var integer
     */
    private $val;

    /**
     * @var integer
     */
    private $datetimeunix;

    /**
     * @var \DateTime
     */
    private $datefrom;

    /**
     * @var string
     */
    private $datetime;

    /**
     * @var integer
     */
    private $frequency;

    /**
     * @var boolean
     */
    private $active;

    /**
     * @var \DateTime
     */
    private $dateupdated;

    /**
     * @var integer
     */
    private $bid;

    /**
     * @var \AppBundle\Entity\Campaigngroups
     */
    private $cgid;


    /**
     * Set opt
     *
     * @param integer $opt
     *
     * @return BotRules
     */
    public function setOpt($opt)
    {
        $this->opt = $opt;

        return $this;
    }

    /**
     * Get opt
     *
     * @return integer
     */
    public function getOpt()
    {
        return $this->opt;
    }

    /**
     * Set visits
     *
     * @param integer $visits
     *
     * @return BotRules
     */
    public function setVisits($visits)
    {
        $this->visits = $visits;

        return $this;
    }

    /**
     * Get visits
     *
     * @return integer
     */
    public function getVisits()
    {
        return $this->visits;
    }

    /**
     * Set val
     *
     * @param integer $val
     *
     * @return BotRules
     */
    public function setVal($val)
    {
        $this->val = $val;

        return $this;
    }

    /**
     * Get val
     *
     * @return integer
     */
    public function getVal()
    {
        return $this->val;
    }

    /**
     * Set datetimeunix
     *
     * @param integer $datetimeunix
     *
     * @return BotRules
     */
    public function setDatetimeunix($datetimeunix)
    {
        $this->datetimeunix = $datetimeunix;

        return $this;
    }

    /**
     * Get datetimeunix
     *
     * @return integer
     */
    public function getDatetimeunix()
    {
        return $this->datetimeunix;
    }

    /**
     * Set datefrom
     *
     * @param \DateTime $datefrom
     *
     * @return BotRules
     */
    public function setDatefrom($datefrom)
    {
        $this->datefrom = $datefrom;

        return $this;
    }

    /**
     * Get datefrom
     *
     * @return \DateTime
     */
    public function getDatefrom()
    {
        return $this->datefrom;
    }

    /**
     * Set datetime
     *
     * @param string $datetime
     *
     * @return BotRules
     */
    public function setDatetime($datetime)
    {
        $this->datetime = $datetime;

        return $this;
    }

    /**
     * Get datetime
     *
     * @return string
     */
    public function getDatetime()
    {
        return $this->datetime;
    }

    /**
     * Set frequency
     *
     * @param integer $frequency
     *
     * @return BotRules
     */
    public function setFrequency($frequency)
    {
        $this->frequency = $frequency;

        return $this;
    }

    /**
     * Get frequency
     *
     * @return integer
     */
    public function getFrequency()
    {
        return $this->frequency;
    }

    /**
     * Set active
     *
     * @param boolean $active
     *
     * @return BotRules
     */
    public function setActive($active)
    {
        $this->active = $active;

        return $this;
    }

    /**
     * Get active
     *
     * @return boolean
     */
    public function getActive()
    {
        return $this->active;
    }

    /**
     * Set dateupdated
     *
     * @param \DateTime $dateupdated
     *
     * @return BotRules
     */
    public function setDateupdated($dateupdated)
    {
        $this->dateupdated = $dateupdated;

        return $this;
    }

    /**
     * Get dateupdated
     *
     * @return \DateTime
     */
    public function getDateupdated()
    {
        return $this->dateupdated;
    }

    /**
     * Get bid
     *
     * @return integer
     */
    public function getBid()
    {
        return $this->bid;
    }

    /**
     * Set cgid
     *
     * @param \AppBundle\Entity\Campaigngroups $cgid
     *
     * @return BotRules
     */
    public function setCgid(\AppBundle\Entity\Campaigngroups $cgid = null)
    {
        $this->cgid = $cgid;

        return $this;
    }

    /**
     * Get cgid
     *
     * @return \AppBundle\Entity\Campaigngroups
     */
    public function getCgid()
    {
        return $this->cgid;
    }
}
